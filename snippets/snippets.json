{
    ".source.tact": {
        "let": {
            "prefix": "let",
            "body": "let ${1:name} = ${2:value};"
        },
        "lett": {
            "prefix": "lett",
            "body": "let ${1:name}: ${2:Int} = ${3:value};"
        },
        "if statement": {
            "prefix": "if",
            "body": "if (${1:condition}) {\n\t$0\n}"
        },
        "if else statement": {
            "prefix": "ife",
            "body": "if (${1:condition}) {\n\t$2\n} else {\n\t$0\n}"
        },
        "while statement": {
            "prefix": "while",
            "body": "while (${1:condition}) {\n\t$0\n}"
        },
        "until statement": {
            "prefix": "until",
            "body": "do {\n\t$0\n} until (${1:condition});"
        },
        "repeat statement": {
            "prefix": "repeat",
            "body": "repeat(${1:index}) {\n\t$0\n}"
        },
        "try expression": {
            "prefix": "try",
            "body": "try {\n\t$0\n}"
        },
        "try catch expression": {
            "prefix": "try-catch",
            "body": "try {\n\t$1\n} catch (e) {\n\t$2\n}"
        }
    }
}
